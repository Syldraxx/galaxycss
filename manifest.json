{
    "manifest_version": 2,
    "name": "Roblox Custom CSS",
    "version": "1.0",
    "description": "Customize the appearance of Roblox website",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
        {
        "matches": ["https://www.roblox.com/*"],
        "css": ["custom.css"]
        }
    ]
}
  